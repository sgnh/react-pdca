{
  "name": "react-pdca",
  "version": "1.0.0",
  "description": "PDCA status icon for React",
  "repository": "sgnh/react-pdca",
  "license": "MIT",
  "author": "Steffen Hansen",
  "files": [
    "PDCA.js",
    "es",
    "umd"
  ],
  "main": "PDCA.js",
  "module": "es/PDCA.js",
  "scripts": {
    "clean": "git clean -fdX .",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src -d .",
    "build:es": "cross-env BABEL_ENV=es babel src -d es",
    "build:umd": "cross-env BABEL_ENV=commonjs webpack src/PDCA.js umd/react-pdca.js",
    "build:umd:min": "cross-env BABEL_ENV=commonjs webpack -p src/PDCA.js umd/react-pdca.min.js",
    "build": "yarn run build:commonjs && yarn run build:es && yarn run build:umd && yarn run build:umd:min"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "webpack": "^2.6.1"
  },
  "keywords": [
    "react",
    "pdca"
  ]
}
